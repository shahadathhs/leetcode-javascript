{
  "name": "leetcode-javascript",
  "version": "0.0.1",
  "description": "JavaScript-only LeetCode questions with solutions, explanations, and concept-based tagging.",
  "main": "dist/index.js",
  "author": "https://github.com/shahadathhs",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "dev": "ts-node --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|json)\"",
    "format:fix": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shahadathhs/leetcode-javascript.git"
  },
  "keywords": [
    "javascript",
    "leetcode",
    "algorithms",
    "data-structures",
    "interview-prep",
    "problem-solving"
  ],
  "bugs": {
    "url": "https://github.com/shahadathhs/leetcode-javascript/issues"
  },
  "homepage": "https://github.com/shahadathhs/leetcode-javascript#readme",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.1.0",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0"
  }
}
